{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://inventory-webapi-dev.azurewebsites.net/api/{*path}",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*", "/assets/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}