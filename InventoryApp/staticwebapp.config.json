{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.ico",
      "/*.json",
      "/api/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://inventory-webapi-dev.azurewebsites.net/api/{*path}",
      "allowedRoles": ["anonymous"]
    }
  ]
}

